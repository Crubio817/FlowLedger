{
  "name": "flowledger-interface",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run --passWithNoTests",
    "gen:api": "openapi-typescript http://localhost:4000/openapi.json -o src/services/types.gen.ts",
    "gen:api:snapshot": "bash scripts/update-api-types.sh",
    "gen:api:from-snapshot": "openapi-typescript api-spec.snapshot.json -o src/services/types.gen.ts",
    "verify:api-types": "bash -c 'TMP=\\$(mktemp); npx openapi-typescript api-spec.snapshot.json -o $TMP --quiet; if ! diff -q $TMP src/services/types.gen.ts >/dev/null; then echo \nAPI types are stale. Run: npm run gen:api:snapshot\n; diff -u src/services/types.gen.ts $TMP | head -n 100; exit 1; fi'",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.15.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@xyflow/react": "^12.0.0",
    "bpmn-js": "^17.8.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.372.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.23.1",
    "react-to-print": "^2.15.0",
    "recharts": "^2.12.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "openapi-typescript": "^7.4.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "vitest": "^1.6.0"
  }
}
