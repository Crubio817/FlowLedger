# FlowLedger Production Environment Configuration

# API Configuration
VITE_API_BASE_URL=https://flowledger-api-web.azurewebsites.net/api
VITE_WEBSOCKET_URL=wss://flowledger-api-web.azurewebsites.net/ws

# Organization Settings
VITE_DEFAULT_ORG_ID=your-org-id-here
VITE_ORG_NAME=Your Organization Name

# People Module Configuration
VITE_PEOPLE_API_ENABLED=true

# Engagements Module Configuration
VITE_ENGAGEMENTS_API_ENABLED=true
VITE_ADVANCED_ENGAGEMENTS_ENABLED=true
VITE_CHANGE_REQUESTS_ENABLED=true
VITE_DEPENDENCY_TRACKING_ENABLED=true

# Billing & Contracts Module Configuration
VITE_BILLING_API_ENABLED=true
VITE_BILLING_API_URL=https://api.yourdomain.com/billing
VITE_BILLING_WS_URL=wss://api.yourdomain.com/billing/ws
VITE_MULTI_CURRENCY_ENABLED=true
VITE_TIME_TRACKING_ENABLED=true
VITE_INVOICE_GENERATION_ENABLED=true
VITE_PAYMENT_PROCESSING_ENABLED=true
VITE_REVENUE_RECOGNITION_ENABLED=true
VITE_COLLECTIONS_ENABLED=true
VITE_EXCHANGE_RATES_ENABLED=true
VITE_BULK_OPERATIONS_ENABLED=true

# Optional: Authentication (if using API keys)
VITE_API_KEY=your-api-key-here

# Optional: Feature Flags
VITE_ENABLE_CANDIDATE_RANKING=true
VITE_ENABLE_RATE_BREAKDOWN=true
VITE_ENABLE_AVAILABILITY_HEATMAP=true
VITE_ENABLE_SKILLS_CHART=true
VITE_ENABLE_REAL_TIME_UPDATES=true
VITE_ENABLE_WEBSOCKET_INDICATORS=true

# Development Settings (set to false in production)
VITE_DEV_MODE=false
